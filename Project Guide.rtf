\par }\pard \ltrpar\ql \li0\ri0\widctlpar\wrapdefault\aspalpha\aspnum\faauto\adjustright\rin0\lin0\itap0\pararsid1845941 {\rtlch\fcs1 \af2\afs20 \ltrch\fcs0 \f2\fs20\insrsid1845941\charrsid1845941 #User prompted to input taxon
\par echo 'Input Taxon:'
\par read userinput
\par taxon=$\{userinput// /%20\}
\par #Check that the user hasn't just entered spaces
\par s=$(echo '$userinput' | wc -c)
\par if [ $s -eq 0 ]
\par \tab then
\par \tab echo 'No taxon entered'
\par \tab exit 1
\par fi
\par #Search for proteome of taxon via NCBI
\par #Organism:exp to receive data from taxon and all subtaxa
\par wget "https://eutils.ncbi.nlm.nih.gov/entrez/eutils/esearch.fcgi?db=protein&term=$\{taxon\}[Organism:exp]&usehistory=y&retmode=json" -O aasearch.txt
\par #Check if the query taxon is correct/exists in the records
\par t=$(grep 'No items found.' aasearch.txt | wc -l)
\par if [ $t -ne 0 ]
\par \tab then
\par \tab echo 'No records for this taxon found. Considering changing spelling and checking that the taxon exists.'
\par \tab exit 1
\par fi
\par #Insert WebEnv from taxon search into an NCBI efetch to download }{\rtlch\fcs1 \af2\afs20 \ltrch\fcs0 \f2\fs20\insrsid15891719 a maximum of 10,000 sequences from proteome}{\rtlch\fcs1 \af2\afs20 \ltrch\fcs0 \f2\fs20\insrsid1845941\charrsid1845941 
\par webenvline=`grep webenv aasearch.txt`
\par userwebenv=`echo $webenvline | cut -f 4 -d '"'`
\par echo $webenvline
\par echo $userwebenv
\par wget "https://eutils.ncbi.nlm.nih.gov/entrez/eutils/efetch.fcgi?WebEnv=$\{userwebenv\}&query_key=1&db=protein&rettype=fasta" -O proteome.fa
\par #Check if the downloaded file is a fasta file
\par q=$(grep '^>' proteome.fa | wc -l)
\par if [ $q -eq 0 ]
\par \tab then 
\par \tab echo 'Error during file download. No fasta files found' 
\par \tab exit 1
\par fi
\par #Remove header from proteome fasta file and place into a new file
\par grep -v '^>' proteome.fa >singleproteome.fa
\par #check if the fasta file contains protein sequences. (This could be improved by adding in features unique to protein sequences).
\par p=$(grep '[ABCDEFGHIJKLMNOPQRSTUVWXYZ*-abcdefghijklmnopqrstuvwxyz]' singleproteome.fa | wc -l)
\par if [ $p -eq 0 ]
\par \tab then 
\par \tab echo 'No protein sequences found.'
\par \tab exit 1
\par fi
\par #Summarise the data using pepstats with the headerless proteome fasta file
\par pepstats singleproteome.fa summary.pepstats
\par #Isolate the amino acid frequency and concentration data only
\par awk '/A = Ala/' RS="\\n\\n" ORS="\\n\\n" summary.pepstats | awk '\{print $1,$4,$5\}' | grep -v 'Residue' | sort -k2 -rn | column -t >table.txt
\par #Count the number of amino acids and protein sequences analysed
\par SequenceCount=`grep '^>' proteome.fa | wc -l`
\par AA_Count=`wc -c < singleproteome.fa`
\par #Could be done use grep -v 'Count' finaltable.txt | awk '\{ sum += $2 \} END \{ print sum \}'
\par 
\par #Insert table headers
\par echo 'Amino_Acid Count Percentage' | cat - table.txt > aatable.txt
\par column -t aatable.txt > temp && mv temp aatable.txt
\par #Output data summary and convert to comma}{\rtlch\fcs1 \af2\afs20 \ltrch\fcs0 \f2\fs20\insrsid2891918  }{\rtlch\fcs1 \af2\afs20 \ltrch\fcs0 \f2\fs20\insrsid1845941\charrsid1845941 delimited .csv file in order to be imported into excel
\par echo > aatable.csv
\par echo 'Amino Acid Usage table for' $userinput':' >> aatable.csv
\par echo >> aatable.csv
\par cat aatable.txt | awk 'BEGIN\{ OFS=","; print "Amino_Acid,Count,Percentage"\}; NR > 1\{print $1, $2, $3;\}' | sed 's/,/:,/g' | column -t -s: | sed 's/ ,/,/g' >> aatable.csv 
\par echo >> aatable.csv
\par echo 'Number of protein sequences analysed within' $userinput':' $SequenceCount >> aatable.csv
\par echo 'Number of amino acids analysed within' $userinput':' $AA_Count >> aatable.csv
\par echo 'Table stored as file: aatable.csv'
\par cat aatable.csv}{\rtlch\fcs1 \af2\afs20 \ltrch\fcs0 \f2\fs20\insrsid792138\charrsid1845941 
